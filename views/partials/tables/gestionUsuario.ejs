<table class="table">
    <thead>
        <tr class="table-header">
          <th class="dateColumn top-left-corner">Fecha</th>
          <th class="nameColumn">Nombre</th>
          <th class="roleColumn">Rol</th>
          <th class="actionsColumn top-right-corner">Acciones</th>
        </tr>
      </thead>
      <tbody>
        <% users.forEach((user, i) => { %>
            <% console.log(user) %> 
          <tr class="table-row-no-hover" id="row-<%= i %>">
            <td class="date"><%= user.fecha_registro.toLocaleDateString() %> </td>
            <td class="user-name"><%= user.nombre %> </td>
            <% switch (user.tecnico) {
              case 0: %>
                <td class="role">USUARIO</i></td>
              <% break;
              case 1: %>
                <td class="role">TÃ‰CNICO</td>
               <% break;
              default:
                break;
            } %> 
            <td class="actions">
              <a href="/users/delete/<%= user.idUsuario %>"><i class="action fa-solid fa-trash fa-xl black trash-icon delete-user"></i></a>
            </td>
          </tr>
       <% }); %> 
      </tbody>
</table>